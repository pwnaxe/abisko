      "target": "api::blog-post.blog-post",
      "inversedBy": "blog_categories"
    },
    "color": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Fiord Blue",
        "Green",
        "Purple",
        "Light Blue",
        "Light Green",
        "Orange"
      ]
    }
  }
}
